class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int n = nums.size();
        int i = 0, j = n - 1, count = 0;
        while(i <= j){
            while(i <= j && nums[i] == val){
                swap(nums[i], nums[j]);
                count++;
                --j;
            }
            ++i;
        }
        return n - count;
    }
};
