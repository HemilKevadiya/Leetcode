class Solution {
public:
    int singleNumber(vector<int>& nums) {
        unordered_map<int,int> map;
        int i,n=nums.size();
        for(i=0;i<n;i++){
            map[nums[i]]++;
        }
        int celeb;
        for(auto j:map){
            if(j.second==1){
                celeb=j.first;
                break;
            }
        }
        return celeb;
    }
};
