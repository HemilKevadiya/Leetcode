class Solution {
public:
    vector<int> singleNumber(vector<int>& nums) {
        unordered_map<int,int> map;
        int i,n=nums.size();
        for(i=0;i<n;i++){
            map[nums[i]]++;
        }
        vector<int> celeb;
        for(auto j:map){
            if(celeb.size()==2){
                break;
            }
            if(j.second==1){
                celeb.push_back(j.first);
            }
        }
        return celeb;
    }
};
