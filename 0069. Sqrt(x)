class Solution {
public:
    int mySqrt(int x) {
        int ans=0;
        if(x==1){
            return 1;
        }
        for(int i=1;i<=x;i++){
            if(((long)i*i)<=x){
                continue;
            }else{
                ans=i-1;
                break;
            }
        }
        return ans;
    }
};
