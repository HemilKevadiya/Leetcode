class Solution {
public:
    void sortColors(vector<int>& nums) {
        vector<vector<int>> colors;
        for(int i=0;i<3;i++){
            colors.push_back(vector<int>());
        }
        for(int i=0;i<nums.size();i++){
            int rem=nums[i]%10;
            colors[rem].push_back(nums[i]);
        }
        nums.clear();
        for(auto row:colors){
            for(auto i:row){
                nums.push_back(i);
            }
        }
    }
};
